{
    "entryPoints": [
        "./src"
    ],
    "entryPointStrategy": "expand",
    "emit": "both",
    "includeVersion": true,
    "out": "../docs/content/docs/typedoc",
    "theme": "hugo",
    "plugin": [
        "typedoc-plugin-markdown",
        "typedoc-plugin-inline-sources",
        "typedoc-hugo-theme",
        "typedoc-plugin-merge-modules"
    ]
}